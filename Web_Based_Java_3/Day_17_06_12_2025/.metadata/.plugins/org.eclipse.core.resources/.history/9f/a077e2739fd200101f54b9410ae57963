package com.studentregistration.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.studentregistration.beans.Student;

import com.studentregistration.dao.StudentDao;

@Service
public class StudentServiceImpl implements StudentService {
	
	@Autowired
	StudentDao sdao;

	@Override
	public List<Student> getAllStudent() {
		// TODO Auto-generated method stub
		return sdao.getAllStudent();
	}

	@Override
	public boolean insertStudent(Student s1) {
		// TODO Auto-generated method stub
		return sdao.insertStudent(s1);
	}

	@Override
	public Student getById(int s1) {
		
		return sdao.getById(s1);
	}

	@Override
	public boolean updateStudent(Student s1) {
		
		return sdao
	}



}
