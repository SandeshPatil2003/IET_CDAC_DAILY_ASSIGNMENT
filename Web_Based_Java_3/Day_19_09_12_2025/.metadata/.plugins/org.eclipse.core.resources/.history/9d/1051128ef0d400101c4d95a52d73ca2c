import org.springframework.stereotype.Service;

import com.demo.SpringBootMVCCURD.beans.VehicleServices;
import com.demo.SpringBootMVCCURD.dao.VehicleServiceDao;

@Service
public class VehicleServiceImpl implements VehicleServiceService{

	@Autowired
	VehicleServiceDao vsdao;
	
	
