package com.demo.SpringBootMVCCURD.dao;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import com.demo.SpringBootMVCCURD.beans.User;


@Repository
public interface LoginDao extends JpaRepository<User, Integer> {

	
	@Query("select * from User u where u.email= :email AND u.password=")
	User validateUser(String uname, String password);

}
