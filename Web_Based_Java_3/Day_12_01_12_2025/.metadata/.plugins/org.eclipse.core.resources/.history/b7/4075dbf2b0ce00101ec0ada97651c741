package com.manytomany.beans;

import java.time.LocalDate;
import java.util.Set;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.ManyToMany;

@Entity(name="Movies101")
public class Movies {
	
		@Id
		@GeneratedValue
		private int mid;
		private String mname;
		private LocalDate rdate;
	
		
		@ManyToMany
		private Set<Actor> aset;


		public Movies() {
			super();
		}


		public Movies(String mname, LocalDate rdate, Set<Actor> aset) {
			super();
			this.mname = mname;
			this.rdate = rdate;
			this.aset = aset;
		}
		
		
		
		
		
}
