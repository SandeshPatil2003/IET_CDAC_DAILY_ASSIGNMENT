package com.manytomany.beans;

import java.util.Set;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.ManyToMany;
@Entity(name="Actor101")

public class Actor {
	@Id
	@GeneratedValue
	private int aid;
	private String fname;
	private String lname;
	
	@ManyToMany(mappedBy="aset")
	private Set<Movie> mset;

	public Actor() {
		super();
	}
	
	

	public Actor(String fname, String lname, Set<Movie> mset) {
		super();
		this.fname = fname;
		this.lname = lname;
		this.mset = mset;
	}
	
	

	public int getAid() {
		return aid;
	}

	public void setAid(int aid) {
		this.aid = aid;
	}

	public String getFname() {
		return fname;
	}

	public void setFname(String fname) {
		this.fname = fname;
	}

	public String getLname() {
		return lname;
	}

	public void setLname(String lname) {
		this.lname = lname;
	}

	public Set<Movie> getMset() {
		return mset;
	}

	public void setMset(Set<Movie> mset) {
		this.mset = mset;
	}
	

	@Override
	public String toString() {
		return "Actor [aid=" + aid + ", fname=" + fname + ", lname=" + lname + "]";
	}
	
	
	
	

}
