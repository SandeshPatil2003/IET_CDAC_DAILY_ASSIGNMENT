package com.filehandling.test;

public class TestSequence {

	public static void main(String[] args) {
	
		int []a = {1,1,2,1,2,3,1,1,1,5,1,1,1,1,1,1};
		
		int cnt=0,pcnt=0;
		int i=0;
		for(;i<a.length-1;i++) {
			
			if(a[i] == 1 && a[i+1] == 1) {
				cnt++;
//				System.out.println(cnt);
			}
			else {
				if(pcnt<cnt){
					//System.out.println(cnt);
					pcnt=cnt;
					//System.out.println(pcnt);
				}
				cnt=1;
			}
		}
		//System.out.println(pcnt);
		System.out.println("i : "+i);
		System.out.println("Max 1 consicative len is : " +cnt);

	}

}
