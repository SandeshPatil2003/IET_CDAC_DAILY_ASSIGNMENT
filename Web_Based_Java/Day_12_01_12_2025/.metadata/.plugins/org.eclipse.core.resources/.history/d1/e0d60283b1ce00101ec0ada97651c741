package com.manytomany.beans;

import java.time.LocalDate;
import java.util.Set;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.ManyToMany;

@Entity(name="Movies101")
public class Movies {
	
		@Id
		@GeneratedValue
		private int mid;
		private String mname;
		private LocalDate rdate;
	
		
		@ManyToMany
		private Set<Actor> aset;


		public Movies() {
			super();
		}


		public Movies(String mname, LocalDate rdate) {
			super();
			this.mname = mname;
			this.rdate = rdate;
			this.aset = aset;
		}


		public int getMid() {
			return mid;
		}


		public void setMid(int mid) {
			this.mid = mid;
		}


		public String getMname() {
			return mname;
		}


		public void setMname(String mname) {
			this.mname = mname;
		}


		public LocalDate getRdate() {
			return rdate;
		}


		public void setRdate(LocalDate rdate) {
			this.rdate = rdate;
		}


		public Set<Actor> getAset() {
			return aset;
		}


		public void setAset(Set<Actor> aset) {
			this.aset = aset;
		}


		@Override
		public String toString() {
			return "Movies [mid=" + mid + ", mname=" + mname + ", rdate=" + rdate + ", aset=" + aset + "]";
		}
		
		
		
		
		
}
