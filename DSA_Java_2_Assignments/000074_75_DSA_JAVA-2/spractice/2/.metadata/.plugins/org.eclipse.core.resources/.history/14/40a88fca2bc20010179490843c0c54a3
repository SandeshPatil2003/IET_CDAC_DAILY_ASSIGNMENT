package com.sortingq.beans;

public class CircularQueue {
	
	
	int [] arr;
	int front;
	int rear;
	
	CircularQueue(int size)
	{
		arr= new int[size];
		
		front=-1;
		rear=-1;
	}
	
	boolean isEmpty()
	{
		if(front==-1 && rear==-1)
		{
			return true;
		}
		
	
		
		return false;
		
	}
	
	boolean isFull()
	{
		 if(front+1==rear)
		 {
			 return true;
		 }
		
