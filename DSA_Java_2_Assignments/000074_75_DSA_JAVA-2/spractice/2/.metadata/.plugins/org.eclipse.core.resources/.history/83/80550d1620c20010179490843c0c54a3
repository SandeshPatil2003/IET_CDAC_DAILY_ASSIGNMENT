package com.sortingq.beans;

public class MyStack {
	
	int [] arr;
	int top;
	
	
	MyStack(int size)
	{
		arr= new int[size];
		top=-1;
		
	}
	
	MyStack()
	{
		arr= new int[10];
		top=-1;
	}
	
	
	void push(int data)
	{
		if(isFull())
		{
			System.out.println(" Stack is Full");
		}
		else {
		top++;
		arr[top]=data;
		}
	}
	
	
	 private boolean isFull() {
		// TODO Auto-generated method stub
		return false;
	}

	int pop()
	{
		 if(isEmpty())
		 {
			 
			 System.out.println("Stack is Empty");
			 return -1;
		 }
		 else {
		  int temp= arr[top];
		 top --;
		 
		return temp;}
	}

}
