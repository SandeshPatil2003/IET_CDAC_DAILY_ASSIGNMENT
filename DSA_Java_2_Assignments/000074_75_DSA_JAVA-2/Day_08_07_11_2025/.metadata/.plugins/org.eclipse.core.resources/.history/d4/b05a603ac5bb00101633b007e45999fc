package com.stackz.beans;

public class Stackz {
	
	int [] arr;
	int top;
	public Stackz() {
		super();
		this.arr = new int[10];
		this.top = -1;
	}
	
	public Stackz(int size) {
		super();
		this.arr = new int[size];
		this.top = -1;
	}
	
	
	
	public void push(int data)
	{
		arr[top]=data;
		top++;
	}
	
	public int pop()
	{
		int returndata = arr[top];
		arr[top]=0;
		top--;
		
		return returndata;
	}
	
	
	public boolean isEmpty()
	{
		return top==-1;
	}
	
	public boolean isFull()
	{
		return top==;
	}
	
	

}
