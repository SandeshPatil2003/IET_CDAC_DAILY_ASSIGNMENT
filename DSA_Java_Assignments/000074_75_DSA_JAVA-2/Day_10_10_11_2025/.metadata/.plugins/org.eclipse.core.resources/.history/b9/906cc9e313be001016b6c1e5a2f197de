package com.hash.beans;

public class Hash {
	
	Node [] heads;
	
	
	public Hash()
	{
		heads=new Node[10];
		for(int i=0;i<10;i++)
		{
			heads[i]=null;
			
		}
	}
	
	
	
	class Node
	{
		int data;
		Node next;
		
		
		public Node(int data)
		{
			this.data=data;
			next=null;
		}
		
		
	}
	
	
	
	
	public void insert(int data)
	{
		Node newNode = new Node (data);
		
		int d=data%10;
		
		
		
		
		if(heads[d]==null)
		{
			heads[d]=newNode;
			
		}
		else {
			
			
			
			newNode.next=heads[d];
			heads[d]=newNode;
			
		}
		
		
	}
	
	
	public void display()
	{
		Node temp= heads[0];
		
		for(int i=0;i<10)
		while(temp!=null)
		{
			if(temp.next!=null)
			System.out.print(temp.data+"-->");
			else {
				System.out.print(temp.data);
			}
			
		}
		System.out.println();
	}

}
