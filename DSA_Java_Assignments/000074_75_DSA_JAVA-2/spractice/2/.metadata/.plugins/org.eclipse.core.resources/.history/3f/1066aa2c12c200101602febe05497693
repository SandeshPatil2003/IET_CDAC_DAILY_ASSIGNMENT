package com.sortingq.beans;

public class DoublyLinkedListz {
	
	Node head;
	
	
	public DoublyLinkedListz()
	{
		head=null;
	}
	
	
	class Node{
		
		int data;
		Node next;
		Node prev;
		
		
		Node(int data)
		{
			this.data=data;
			next=null;
			prev=null;
		}
	}


	public void addAtEndD(int data) {
		
		Node newNode = new Node(data);
		
		if(head==null)
		{
			
			head=newNode;
		}
		else {
			
			Node temp= head;
			
			
			while(temp.next!=null)
			{
				temp=temp.next;
				
			}
			
			
			newNode.next=temp.next;
			newNode.prev=temp;
			temp.next=newNode;
			
			
			
			
			
			
		}
		
	}


	public void display() {
		
		
		Node temp = head;
		
		while(temp!=null)
		{
			
			if(temp)
		}
		
		
		
		
	}

}
