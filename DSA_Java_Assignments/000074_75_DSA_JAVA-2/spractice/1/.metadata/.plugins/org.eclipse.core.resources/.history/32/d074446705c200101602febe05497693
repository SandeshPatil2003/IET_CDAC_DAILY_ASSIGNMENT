package com.sortingq.beans;

public class LinkedListz {

	Node head;
	
	
	LinkedListz()
	{
		head=null;
		
	}
	
	class Node{
		
		int data;
		Node next;
		
		
		Node(int data)
		{
			this.data=data;
			next=null;
		}
	}
	
	
	
	public void addAtEnd(int data)
	{
		Node temp=head;
		
		Node newNode = new Node(data);
		
		
		if(head==null)
		{
			head=newNode;
		}
		else {
			
			
			while(temp.next!=null)
			{
				temp=temp.next;
			}
			
			temp.next=newNode;
			
			
			
		}
	}
	
	
	
	public void display()
	{
		Node temp =head;
		
		
		while(temp!=null)
		{
			if(temp.next!=null)
			System.out.println(temp.data+"-->");
			else
			{
				
			}
			
		}
	}
	
	
	
	
	
}


