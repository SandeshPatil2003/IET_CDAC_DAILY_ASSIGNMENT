package com.list.beans;

public class SinglyLinkedList {
	
	
	
   Node head;
	
	
	
	public SinglyLinkedList(Node head) {
	super();
	this.head = head;
}



	class Node{
		
		int data;
		Node next;
		
		
		public Node() {
			super();
		}


//		public Node() {
//			super();
//			this.data = data;
//			this.next = null;
//		}
		
		
		public Node(int data) {
			super();
			this.data = data;
			this.next = null;
		}
		
		
		
	}
	
	
	
	
	
}
