package com.search.service;

public class SearchService {

	
	
	public int seq(int[] arr, int val)
	{
		
			
			for(int i=0;i<arr.length;i++ )
			{
				if(arr[i]==val)
					return i;
			}
		
		return -1;
	}
	
	public int bin(int[] arr, int val)
	{
		int low = 0;
		int high = arr.length-1;
		int cnt=0;
		
		
		
		while(low<=high)
		{	
			int mid = (low+high)/2;
		
			if (arr[mid]==val)
				return mid;
		 if(arr[mid]<val)
			 high=mid-1;
		 
		 else if (arr[mid]>val){
				 low=mid-1;
			 
		 }
		 
			
		}
	}
}
