package com.arrays.test;

import com.arrays.beans.*;

public class TestArray {

	public static void main(String[] args) {
		
		MyArray o= new MyArray();
		
//		o.accept();
		o.add(4);
		o.add(5);
		o.add(12);
		o.add(18);
		o.add(3);
		
		o.add(9);
		System.out.println(o);
		
		o.deleteByPos(3);
		System.out.println("o.deleteByPos(3): "+o);
		o.add(17);
		System.out.println("o.add(17): "+o);
		o.deleteByVal(17);
		System.out.println("o.deleteByVal(17): "+o);
		
		o.add(3,270);
		System.out.println("o.add(3,270): "+o);
		
		o.search(3);
		
		System.out.println("o.search(3): "+o);
		
		o.rotate(true, 2);
		
		
		
		
		
		


	}

}
