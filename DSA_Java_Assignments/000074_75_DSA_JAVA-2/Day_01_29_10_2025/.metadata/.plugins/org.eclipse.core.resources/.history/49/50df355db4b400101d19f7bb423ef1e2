package com.arrays.beans;

public class My2DArray {
	
	
	private int [][] arr;
	
	

	public int[][] getArr() {
		return arr;
	}

	public void setArr(int[][] arr) {
		this.arr = arr;
	}

	public My2DArray(int[][] arr) {
		super();
		this.arr = arr;
	}
	
	public My2DArray() {
		super();
		
		
		arr = new int [3][3];
	}
	
	public void accept()
	{
		for(int i=0;i<arr.length;i++)
		{
			for(int i=0;i<arr.length;i++)
			{
			
		}
	}
	
	
	
	

}
